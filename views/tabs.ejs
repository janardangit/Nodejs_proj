                        <li><label style="float:left;" for="filter_reclassify_kve_classify_unit_select_tab">Reclassify Unit Select</label><input name="unit_select_check" id="filter_reclassify_kve_classify_unit_select_tab" type="checkbox" style="float:left; margin-top:-5px;" onchange="TASRightView.gbl.select_all_classify_unit(this)"/></li>
                    </ul>
                 </div>
                 <div id="filter_reclassify_main_div1" style="float:left; width:100%; height:96%;position:relative; overflow:auto;">
                    <div id="filter_reclassify_Con_Div" style="float:left; width:100%;position:relative;overflow:auto;"></div>
                 </div>
                </div>
                <iframe style="display:none;" id="iframe_filter" name="iframe_filter" width="100%" height="100%" onload="" ></iframe>
