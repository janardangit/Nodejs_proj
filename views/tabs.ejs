                    <span id="filter_de-duplication_header-content"></span>
                    <ul>
                        <li id="filter_de-duplication_kve_delete_tab"  onclick="TASRightView.gbl.delete_filter_data();">Delete</li>
                        <li id="filter_de-duplication_kve_delete_tab"  onclick="TASRightView.gbl.restore_filter_data();">Restore</li>
                    </ul>
                  </div>
                  <div id="filter_de-duplication_main_div1" style="float:left; width:100%; height:96%;top:25px;">
                    <div id="filter_de-duplication_Con_Div" style="position:absolute; left:0px; top:0px;"></div>
                 </div>
                </div>
                <div id="filter_reclassify_main_div" style="display:none;float:left; width:100%; height:100%; overflow:hidden; background: linear-gradient(#FFF, #959595) repeat scroll 0% 0% transparent;">
