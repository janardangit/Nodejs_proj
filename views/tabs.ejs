                    <div id="tab-right" style="display:none;">
                        <ul id="right_all_action_lst">
                           <li id="kve_merge_tab" onclick="TASRightView.gbl.merge_filter_row(this);">Merge</li>
                           <li id="kve_split_tab" onclick="TASRightView.gbl.split_filter_row(this);">Split</li>
                           <li id="kve_dulicate_tab" onclick="TASRightView.gbl.make_duplicate_copy(this);">MakeDuplicate</li>
                           <li id="kve_map_tab" onclick="TASRightView.gbl.map_taxonomy(this);">ReclassifyTaxonomy</li>
                           <li id="kve_delete_tab" onclick="TASRightView.gbl.delete_filter_row(this);">Delete</li>
                           <li id="kve_lstrip_tab" onclick="TASRightView.gbl.strip_filter_row(1);">Lstrip</li>
                           <li id="kve_rstrip_tab" onclick="TASRightView.gbl.strip_filter_row(2);">Rstrip</li>
                           <li id="kve_topic_save_tab" onclick="TASRightView.gbl.save_copy(this);">Save</li>
                           <li id="kve_full_view_tab" onclick="TASRightView.gbl.load_full_view(this);">FullView</li>
                           <li id="kve_history_tab" onclick="TASRightView.gbl.show_reclaim(this);">Reclaim</li>
                           <li id="kve_restore_reclassify_tab" onclick="TASRightView.gbl.show_restore_reclassify(this);">RestoreReclassify</li>
                           <li id="kve_reclassification_tab" onclick="TASRightView.gbl.show_reclassification(this);" style="display:none;">Reclassification</li>
                           <li id="kve_activate_tab" style="display:none" onclick="TASRightView.gbl.activate_group(this);">Activate</li>
                           <li id="kve_activate_src_tab" style="display:none" onclick="TASRightView.gbl.activate_src_group(this);">Activate Src</li>
                           <li id="kve_activate_dest_tab" style="display:none" onclick="TASRightView.gbl.activate_dest_group(this);">Activate Dest</li>
