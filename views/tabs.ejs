                <div id="filter_content_main" style="float:left; width:100%; height:37%; position:relative; overflow:auto;">
                   <div id="delimiter_tab" class="delimiter_tab_div">
                        <div id="delimiter_tab_div_inner" class="delimiter_tab_div_inner">
                            <span class="Delimiter">,</span>
                            <span class="Delimiter">;</span>
                            <span class="Delimiter">.</span>
                            <span class="Delimiter">:</span>
                            <span class="Delimiter">&amp;</span>
                            <span class="Delimiter">+</span>
                            <span class="Delimiter">*</span>
                            <span class="Delimiter">(</span>
                            <span class="Delimiter">)</span>
                            <span class="Delimiter">Space</span>
                            <span class="Delimiter">– (Dash)</span>
                            <span class="Delimiter">- (Hyphen)</span>
                            <span class="Delimiter">— (Mega Dash)</span>
                            <span class="Delimiter">_ (Underscore)</span>
                            <input class="filter_input_btn" id="strip_char" size="8" type="text" value="" placeholder="User Input" unselectable="off" onfocus="TASRightView.gbl.setFocus(this);"/>
                            <input class="filter_input_btn" id="pos_text" size="4" type="text" value="" placeholder="Pos" onfocus="TASRightView.gbl.setFocusPos(this);"/>
                            <input class="filter_input_btn" id="enable_delimer_text" type="checkbox" style="float:left; margin-top:6px;"/>
                        </div>
                   </div>
                   <div id="filter_content" style="float:left; width:100%;  position:relative;overflow:auto;"></div>
                <div id="filter_de-duplication_main_div" style="display:none;float:left; width:100%; height:100%; overflow:hidden; background: linear-gradient(#FFF, #959595) repeat scroll 0% 0% transparent;">
