                    <div id="filter_level_Con_Div" style="float:left;width:100%;height:100%;"> </div>
                </div>
                <div id="spliter_Y" style="float:left; width:100%; height:25px; position:relative;background:url(images/top_headbg.png) repeat-x scroll 0 0 rgba(0, 0, 0, 0);" ondblclick="TASRightView.gbl.reset_block(this);">
