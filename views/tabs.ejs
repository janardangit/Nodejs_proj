                           <li id="kve_pagination" style="display:none;"><select id="kve_drop_down_pagination_select" style="width: 80px;"></select></li>
                           <li id="kve_apply_auto_tab" style="display:none;"><label style="float:left;" for="kve_apply_auto_select">AutoSelect</label><input id="kve_apply_auto_select" type="checkbox" style="float:left; margin-top:-5px;" onchange="TASRightView.gbl.apply_across_document(this)"/></li>
